<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<Version>21.5</Version>
		<AssemblyName>guid-generator</AssemblyName>
		<StrongNameKeyId>GuidGenCli</StrongNameKeyId>
		<SignAssembly>true</SignAssembly>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<RootNamespace>GuidGenerator</RootNamespace>
		<PackageId>GuidGenerator.Cli</PackageId>
		<IsPackable>true</IsPackable>
		<PackAsTool>true</PackAsTool>
		<ToolCommandName>guid-gen</ToolCommandName>
		<PackageOutputPath>./nupkg</PackageOutputPath>
		<NuspecProperties>version=$(Version)</NuspecProperties>
		<NuspecFile>test-cli.nuspec</NuspecFile>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	</PropertyGroup>

</Project>
